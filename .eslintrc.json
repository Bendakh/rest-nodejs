{
  "extends": "@algoan/eslint-config",
  "rules": {
    "@typescript-eslint/naming-convention": [
      "error", {
        "selector": "parameterProperty", "format": null
      }
    ],
    "camelcase": [
      "error", {
        "properties": "never",
        "allow": ["interface"]
      }
    ]
  }
}